{
    "name": "drupal-composer/drupal-project",
    "description": "Project template for Drupal 8 projects with composer",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "^1.6",
        "drupal-composer/drupal-scaffold": "^2.2",
        "drupal/admin_toolbar": "^1.23",
        "drupal/adminimal_admin_toolbar": "^1.7",
        "drupal/adminimal_theme": "^1.3",
        "drupal/ajax_comments": "^1.0@beta",
        "drupal/amazon_sns": "^1.0",
        "drupal/auto_entitylabel": "^2.1@beta",
        "drupal/autocomplete_deluxe": "^1.0@beta",
        "drupal/backup_migrate": "^4.0",
        "drupal/captcha": "^1.0@beta",
        "drupal/chosen": "^2.5",
        "drupal/clamav": "^1.0",
        "drupal/clientside_validation": "^1.0@RC",
        "drupal/collapsiblock": "^2.0",
        "drupal/computed_field": "^2.0@alpha",
        "drupal/conditional_fields": "^1.0@alpha",
        "drupal/config_update": "^1.5",
        "drupal/console": "^1.0.2",
        "drupal/contentimport": "^4.1",
        "drupal/core": "~8.5.3",
        "drupal/cshs": "^1.0@beta",
        "drupal/csv_serialization": "^1.3",
        "drupal/ctools": "^3.0",
        "drupal/custom_add_another": "^1.0@beta",
        "drupal/dbug": "^1.0",
        "drupal/delete_all": "^1.0",
        "drupal/devel": "^1.2",
        "drupal/diff": "^1.0@RC",
        "drupal/drush": "^6.0@RC",
        "drupal/ds": "^3.1",
        "drupal/editor_advanced_link": "^1.4",
        "drupal/empty_fields": "^1.0@alpha",
        "drupal/entity_reference_revisions": "^1.4",
        "drupal/entity_reference_views_select": "^1.3",
        "drupal/eu_cookie_compliance": "^1.0",
        "drupal/features": "^3.7",
        "drupal/field_group": "^1.0",
        "drupal/field_permissions": "^1.0@RC",
        "drupal/field_validation": "^1.0@alpha",
        "drupal/filter_perms": "1.x-dev",
        "drupal/flag": "^4.0@alpha",
        "drupal/force_password_change": "^1.3",
        "drupal/form_mode_control": "^2.0@RC",
        "drupal/form_mode_manager": "^1.3",
        "drupal/formblock": "^1.0@beta",
        "drupal/gdpr": "^1.0@alpha",
        "drupal/google_analytics": "^2.2",
        "drupal/linkit": "^4.3",
        "drupal/login_emailusername": "^1.1",
        "drupal/mail": "^1.0",
        "drupal/mailsystem": "^4.1",
        "drupal/markup": "^1.0@beta",
        "drupal/masquerade": "^2.0@beta",
        "drupal/menu_export": "^1.2",
        "drupal/migrate_plus": "^2.0",
        "drupal/migrate_source_csv": "^2.1",
        "drupal/migrate_tools": "^2.0",
        "drupal/mimemail": "^1.0@alpha",
        "drupal/multiple_fields_remove_button": "^1.0@alpha",
        "drupal/oauth2_server": "^1.0@beta",
        "drupal/page_manager": "^4.0@beta",
        "drupal/panelizer": "^4.1",
        "drupal/panels": "^4.3",
        "drupal/paragraphs": "^1.2",
        "drupal/paragraphs_edit": "^2.0@alpha",
        "drupal/pathauto": "^1.2",
        "drupal/php": "^1.0@beta",
        "drupal/private_files_download_permission": "^2.0",
        "drupal/prlp": "^1.3",
        "drupal/recaptcha": "^2.3",
        "drupal/redirect": "^1.2",
        "drupal/restui": "^1.15",
        "drupal/rules": "^3.0@alpha",
        "drupal/smtp": "^1.0@beta",
        "drupal/swiftmailer": "^1.0@beta",
        "drupal/taxonomy_manager": "^1.0@alpha",
        "drupal/term_csv_export_import": "^1.0@alpha",
        "drupal/token": "^1.1",
        "drupal/token_filter": "^1.0@beta",
        "drupal/tour_ui": "^1.0@alpha",
        "drupal/typed_data": "^1.0@alpha",
        "drupal/viewfield": "^3.0@alpha",
        "drupal/views_bulk_operations": "^2.3",
        "drupal/views_contextual_filters_or": "^1.0",
        "drupal/views_data_export": "^1.0@beta",
        "drupal/views_field_view": "^1.0@beta",
        "drupal/views_slideshow": "^4.6",
        "drupal/webform": "^5.0@RC",
        "drupal/xls_serialization": "^1.0@beta",
        "drush/drush": "^9.0.0",
        "phpoffice/phpspreadsheet": "^1.2",
        "vlucas/phpdotenv": "^2.4",
        "webflo/drupal-finder": "^1.0.0",
        "webmozart/path-util": "^2.3"
    },
    "require-dev": {
        "webflo/drupal-core-require-dev": "~8.5.3"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "platform": {
          "php": "7.2"
        }
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": ["load.environment.php"]
    },
    "scripts": {
        "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/contrib/{$name}": ["type:drupal-drush"]
        }
    }
}
